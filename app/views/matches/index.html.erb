

<table>
	<thead>
		<tr>													
			<th> Match Name </th>
			<th> Game </th>
			<th> </th>
		</tr>
	</thead>

	<tbody>
		<% @matches.each do |match| %>
			<tr>
				<td> <%= link_to match.name, match %></td>
				<td> <%= match.game.name %></td>
				<td> <%= link_to "Edit", edit_match_path(match.id) %> </td>
			</tr>
		<% end %>	
	</tbody>

</table>




